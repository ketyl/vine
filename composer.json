{
    "name": "ketyl/vine",
    "description": "Simple PHP framework.",
    "keywords": [
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Zak Nesler",
            "email": "zak@nesler.dev",
            "homepage": "https://nesler.dev"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Ketyl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ketyl\\Vine\\Tests\\": "tests"
        },
        "files": [
            "src/Vine/Support/helpers.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "stan": "vendor/bin/phpstan analyze src --level=8"
    },
    "config": {
        "sort-packages": true
    }
}
